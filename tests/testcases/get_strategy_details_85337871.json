{
  "details": {
    "summary": "Get strategy details",
    "operationId": "strategies.getStrategy",
    "tags": [
      "strategies"
    ],
    "loopSource": "{{strategies}}"
  },
  "validation": {
    "assertions": [
      {
        "source": "statusCode",
        "operator": "equalToNumber",
        "expectedValue": "200"
      },
      {
        "source": "textBody",
        "property": null,
        "operator": "Default Validator",
        "expectedValue": ""
      }
    ],
    "expectedSchema": {
      "content": {
        "$ref": "#/components/schemas/GetStrategyResponse"
      },
      "type": "json"
    },
    "expectedBody": {
      "content": {
        "strategy": {
          "id": "{{strategies.$.id}}",
          "type": "StrategyV1",
          "strategyType": "LENDING",
          "coinTypes": [
            "{{strategies.$.coinTypes[0]}}"
          ],
          "minDeposit": [
            {
              "coinType": "{{strategies.$.minDeposit.0.coinType}}",
              "amount": "{{*}}"
            }
          ],
          "apy": {
            "current": "{{*}}",
            "avg24h": "{{*}}",
            "avg7d": "{{*}}",
            "avg30d": "{{*}}"
          },
          "depositorsCount": "{{*}}",
          "tvlUsd": "{{*}}",
          "volume24hUsd": "{{*}}",
          "fees": {
            "depositBps": "0",
            "withdrawBps": "0"
          }
        }
      },
      "type": "json"
    }
  },
  "request": {
    "url": "https://{{site}}v1/strategies/{{strategies.$.id}}",
    "method": "GET",
    "queryParams": [
      {
        "name": "strategyId",
        "value": "{{strategies.$.id}}"
      }
    ]
  }
}